pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/paragkulkarni4549/simple-java-maven-app.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean install'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
